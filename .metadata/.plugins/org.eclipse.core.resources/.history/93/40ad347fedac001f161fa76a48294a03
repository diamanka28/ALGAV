from utilitaire import patricia_util

class PatriciaNode(object):
    '''
    cette classe repr√©sente le noeud de l'arbre de patricia
    '''

    def __init__(self, key="", sibling = None, child = None ):
        '''
        Constructor
        ''' 
        self.key = key
        self.sibling = sibling
        self.child = child
    
    